
<div class="text-center">
  <h3>VMware Meter Login</h3>
  <br>
</div>

<p>Please enter the password for the <strong>core</strong> user account.</p>

<%= form_tag sessions_create_url do %>
<div class="form-group">
  <%= password_field_tag :password, nil, class: 'form-control' %>
</div>

<%= hidden_field_tag :original_url, @original_url %>

<div class="actions text-center">
  <%= button_tag "Login", class: 'btn btn-success' %>
</div>

<% end %>
